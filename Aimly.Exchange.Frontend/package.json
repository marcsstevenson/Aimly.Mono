{
  "name": "auth0-react-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run relay && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "spa": "react-scripts start",
    "tailwind": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "relay": "relay-compiler"
  },
  "relay": {
    "src": "./src/",
    "schema": "./aimly-backend.schema.graphql",
    "artifactDirectory": "./src/__generated__",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.2.0",
    "@azure/communication-calling": "^1.3.2",
    "@azure/communication-common": "^1.1.0",
    "@azure/communication-react": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@opentelemetry/tracing": "^0.24.0",
    "@tailwindcss/forms": "^0.4.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-relay": "^13.0.0",
    "@types/react-router-dom": "^5.3.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "helmet": "^3.18.0",
    "jwks-rsa": "^1.4.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-relay": "^13.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.0",
    "reactstrap": "^9.0.1",
    "relay-runtime": "^13.0.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "autoprefixer": "^10.4.0",
    "babel-plugin-relay": "^13.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.4",
    "postcss": "^8.4.5",
    "relay-compiler": "^13.0.1",
    "relay-compiler-language-typescript": "^15.0.0",
    "tailwindcss": "^3.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
